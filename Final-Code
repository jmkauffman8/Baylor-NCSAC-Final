```{r}

bball_season_data_filtered_three <- bball_season_data_filtered %>% filter(shottype == "3 Pts")  %>% count(offense__name)
bball_season_data_filtered_corner <- bball_season_data_filtered %>% filter(shottype == "3 Pts") %>% filter(shotX < -200 | shotX > 200 ) %>% filter(shotY < 50 ) %>% group_by(offense__name) %>% count(offense__name)

bball_season_data_filtered_corner_percent <- bball_season_data_filtered %>% filter(shottype == "3 Pts") %>% filter(shotX < -200 | shotX > 200 ) %>% filter(shotY < 50 ) %>% group_by(offense__name) %>% count(description)

cornerthree_percent_of_shots<- merge(bball_season_data_filtered_three, bball_season_data_filtered_corner, by = "offense__name")

cornerthree_percent_of_shots$percentthrees <- cornerthree_percent$n.y / cornerthree_percent$n.x
bball_season_data_filtered_corner_percent
bball_season_data_filtered_corner_percent <- aggregate(n ~offense__name,bball_season_data_filtered_corner_percent, function(x) x[1]/(x[1]+x[-1]))
bball_season_data_filtered_corner_percent
cornerthree_percent_of_shots

cornerthrees<- merge(bball_season_data_filtered_corner_percent, cornerthree_percent_of_shots, by = "offense__name")

colnames(cornerthrees)[2] ="Percent of Corner Threes Made"
colnames(cornerthrees)[5] ="Percent of Threes taken that are from the corner"
cornerthrees <- data.frame(cbind(cornerthrees[,c(1,2,5)]))
cornerthrees <- cornerthrees %>% arrange(desc(Percent.of.Threes.taken.that.are.from.the.corner))
cornerthrees$standinginconference <- c(7, 1, 2, 6, 5, 4, 3, 8, 10, 9)
cornerthrees$conferencewins <- c(7, 14, 14, 8, 9, 10, 12, 7, 4, 5)
cornerthrees
reg3 <- lm(conferencewins ~ Percent.of.Threes.taken.that.are.from.the.corner, cornerthrees)
vcov3 <- vcovHC(reg3, type="HC1")
coeftest(reg3, vcov3)
test <- linearHypothesis(reg3, "Percent.of.Threes.taken.that.are.from.the.corner= 0", vcov = vcov3)
cat("With an f-statistic of", sqrt(test$F[2]), "and a p-value of ", 2*(pt(sqrt(test$F[2]), sum(complete.cases(cornerthrees))-1, lower=FALSE)), "lower than the significance leve of 5%, I reject the null hypothesis.")
stargazer(reg3,
          title="Regression", align=TRUE, omit.stat="f", # aesthetics
          type="text", out="regtable.txt") # output type and filename
colnames(cornerthrees)[2] = "3P% on Corner Threes"
colnames(cornerthrees)[3] = "3P% on Non-corner Threes"
cornerthrees
```
```{r}
bball_season_data_r <- bball_season_data_filtered %>% filter(shotX < -60 | shotX > 60 | shotY > 170 ) %>% group_by(defense__name) %>% count(defense__name)
colnames(bball_season_data_r)[2] <- "Shots from mid-range and 3"
bball_season_data_l <- bball_season_data_filtered %>% group_by(defense__name) %>% count(defense__name)
colnames(bball_season_data_l)[2] <- "All shots"
bball_season_data_lr <- merge(bball_season_data_r, bball_season_data_l, by = "defense__name")
bball_season_data_lr$sharemidandthree <- bball_season_data_lr$`Shots from mid-range and 3` / (bball_season_data_lr$`Shots from mid-range and 3`+bball_season_data_lr$`All shots`)
bball_season_data_lr <- bball_season_data_lr %>% arrange(-bball_season_data_lr$sharemidandthree)
bball_season_data_lr$conferencewins <- c(14, 7, 14, 5, 12, 7, 10, 9, 4, 8)
bball_season_data_lr
```
```{r}
bball_season_data_agg <- as.data.frame(read_excel("big12leaders.xlsx")) 
bball_season_data_agg
reg1 <- lm(Wins ~ Mid_rate + Mid_FG + Three_FG + Rim_FG, data = bball_season_data_agg)
stargazer(reg1,
          title="Midrange Regression and FG%'s Effects on Wins", align=TRUE, omit.stat="f", # aesthetics
          type="text", out="regtable.txt") # output type and filename

ggplot(bball_season_data_agg, aes(x = Mid_rate, y = Mid_FG, size = Wins)) + geom_point(aes(size = Wins, fill = Wins), alpha = 0.7) +
scale_color_gradient(low = "#FF0000", high = "#00FF00") + ggtitle("Wins increase with more frequent/effective mid-range shooting")+ labs( x= "Mid-Range Possessions per Game", y = "Mid-Range FG%", size = "Number of Conference Wins")
```


```{r}
bball_season_data_filtered_texas2 <- subset(bball_season_data_filtered, bball_season_data_filtered$offense__name %in% c('TexasAustin'))
bball_season_data_filtered_texas2

bball_season_data_filtered_texas5 <- subset(bball_season_data_filtered, bball_season_data_filtered$defense__name %in% c('TexasAustin'))
bball_season_data_filtered_texas5

bball_season_data_filtered_texas4 <- bball_season_data_filtered_texas2 %>% filter(shottype == "3 Pts") %>% group_by(description) %>% count(offense__name)

bball_season_data_filtered_texas4

bball_season_data_filtered_texas3 <- bball_season_data_filtered_texas2 %>% filter(shottype == "2 Pts")
bball_season_data_filtered_texas3

total <- 0
total_opp <- 0
wins <- 0
for (i in 1:50000) {
n <- sample(nrow(bball_season_data_filtered_texas2),51,replace=T)
m <- sample(nrow(bball_season_data_filtered_texas2),50,replace=T)
b <- bball_season_data_filtered_texas2[n,]
m <- bball_season_data_filtered_texas5[n,]
b$description[b$description == 'Miss'] <- NA
m$description[m$description == 'Miss'] <- NA
b <- na.omit(b)
m <- na.omit(m)
b$shotvalue <- as.numeric(substr(b$shottype,1,1))
m$shotvalue <- as.numeric(substr(m$shottype,1,1))
total <- total + sum(b$shotvalue) + 12.3888
total_opp <- total_opp + sum(m$shotvalue) + 11.8888
if (total > total_opp){
  wins <- wins + 1
}
total <- 0
total_opp <- 0
}

cat("Shooting 27% of their threes from the corner, my Monte Carlo simulation that draws from Texas' 925 offensive plays and 906 defensive plays expects Texas to win ", wins / 50000 * 18 , " of their games.
    ")

total <- 0
total_opp <- 0
wins1 <- 0
for (i in 1:50000) {
n <- sample(nrow(bball_season_data_filtered_texas2),51,replace=T)
m <- sample(nrow(bball_season_data_filtered_texas2),50,replace=T)
b <- bball_season_data_filtered_texas2[n,]
m <- bball_season_data_filtered_texas5[n,]
b$description[b$description == 'Miss'] <- NA
m$description[m$description == 'Miss'] <- NA
b <- na.omit(b)
m <- na.omit(m)
b$shotvalue <- as.numeric(substr(b$shottype,1,1))
m$shotvalue <- as.numeric(substr(m$shottype,1,1))
total <- total + sum(b$shotvalue) + 13.3988
total_opp <- total_opp + sum(m$shotvalue) + 11.8888
if (total > total_opp){
  wins1 <- wins1 + 1
}
total <- 0
total_opp <- 0
}
cat("Shooting 40% of their threes from the corner, my Monte Carlo simulation that draws from Texas' 925 offensive plays and 906 defensive plays expects Texas to win ", wins1 / 50000 * 18 , " of their games")



bball_season_data_filtered_texas22 <- subset(bball_season_data_filtered, bball_season_data_filtered$defense__name %in% c('TexasAustin'))
bball_season_data_filtered_texas22
```

